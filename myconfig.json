{
    "test_name": "example_test",
    "test_duration_in_seconds": 120,
    "random_seed": 100,
    "blocking_cli": false,
    "instances":{
        "instance1":{
            "application": "sentiment",
            "distribution": "Poisson",
            "param_file": "functions/sentiment-analysis/declaration.json",
            "rate": 4
        },
        "instance2":{
            "application": "ocr-img",
            "distribution": "Uniform",
            "data_file":"functions/ocr-img/linuxboot.jpg",
            "rate": 1
        },
        "instance3":{
            "application": "markdown2html",
            "param_file": "functions/markdown-to-html/openpiton-readme.json",
            "distribution": "Poisson",
            "rate": 6
        },
        "instance4":{
            "application": "img-resize",
            "distribution": "Uniform",
            "data_file":"functions/img-resize/libertybell.jpg",
            "rate": 5
        }
    },
    "perf_monitoring":{
        "runtime_script": "monitoring/RuntimeMonitoring.sh",
        "post_script": "./WorkloadAnalyzer -r -p -s -v"
    }
}
